                  "</script>",
                  "`;",
                  "",
                  "function constructVisualizerPayload() {",
                  "    var statusCode = pm.response.code;",
                  "    var statusText = pm.response.status;",
                  "    var isSuccess = statusCode >= 200 && statusCode < 300;",
                  "    ",
                  "    var payload = {",
                  "        statusCode: statusCode,",
                  "        statusText: statusText,",
                  "        statusClass: isSuccess ? 'status-success' : (statusCode >= 400 ? 'status-error' : 'status-info'),",
                  "        codeClass: isSuccess ? 'code-success' : 'code-error',",
                  "        message: '',",
                  "        hasArrayData: false,",
                  "        hasTableData: false,",
                  "        showRawJson: false,",
                  "        tableData: [],",
                  "        chartData: null,",
                  "        rawJson: ''",
                  "    };",
                  "    ",
                  "    // Try to parse JSON response",
                  "    var responseData = null;",
                  "    try {",
                  "        responseData = pm.response.json();",
                  "    } catch (e) {",
                  "        // Response is not JSON",
                  "        var textResponse = pm.response.text();",
                  "        if (textResponse && textResponse.trim()) {",
                  "            payload.message = textResponse;",
                  "        } else {",
                  "            payload.message = isSuccess ? 'Resource deleted successfully!' : 'Request failed';",
                  "        }",
                  "        return payload;",
                  "    }",
                  "    ",
                  "    // Handle different response structures",
                  "    if (responseData === null || responseData === undefined) {",
                  "        payload.message = isSuccess ? 'Resource deleted successfully!' : 'Request failed';",
                  "        return payload;",
                  "    }",
                  "    ",
                  "    // Check for message field",
                  "    if (responseData.message) {",
                  "        payload.message = responseData.message;",
                  "    } else if (responseData.msg) {",
                  "        payload.message = responseData.msg;",
                  "    } else if (responseData.status) {",
                  "        payload.message = responseData.status;",
                  "    } else if (typeof responseData === 'string') {",
                  "        payload.message = responseData;",
                  "        return payload;",
                  "    }",
                  "    ",
                  "    // Check if response contains array data for charting",
                  "    var arrayKey = null;",
                  "    var arrayData = null;",
                  "    ",
                  "    if (Array.isArray(responseData)) {",
                  "        arrayData = responseData;",
                  "    } else {",
                  "        // Look for array properties in the response",
                  "        for (var key in responseData) {",
                  "            if (Array.isArray(responseData[key]) && responseData[key].length > 0) {",
                  "                arrayKey = key;",
                  "                arrayData = responseData[key];",
                  "                break;",
                  "            }",
                  "        }",
                  "    }",
                  "    ",
                  "    // If we have array data, prepare chart",
                  "    if (arrayData && arrayData.length > 0) {",
                  "        payload.hasArrayData = true;",
                  "        var labels = [];",
                  "        var values = [];",
                  "        ",
                  "        // Try to extract meaningful data for chart",
                  "        arrayData.slice(0, 10).forEach(function(item, index) {",
                  "            if (typeof item === 'object' && item !== null) {",
                  "                // Find a label field",
                  "                var label = item.name || item.title || item.id || item.label || ('Item ' + (index + 1));",
                  "                labels.push(String(label));",
                  "                ",
                  "                // Find a numeric value",
                  "                var value = item.value || item.count || item.amount || item.quantity || item.price || 1;",
                  "                values.push(typeof value === 'number' ? value : 1);",
                  "            } else if (typeof item === 'number') {",
                  "                labels.push('Item ' + (index + 1));",
                  "                values.push(item);",
                  "            } else {",
                  "                labels.push(String(item));",
                  "                values.push(1);",
                  "            }",
                  "        });",
                  "        ",
                  "        payload.chartData = {",
                  "            labels: labels,",
                  "            values: values,",
                  "            datasetLabel: arrayKey || 'Data'",
                  "        };",
                  "    }",
                  "    ",
                  "    // Create table data for object properties",
                  "    if (typeof responseData === 'object' && !Array.isArray(responseData)) {",
                  "        var tableData = [];",
                  "        for (var prop in responseData) {",
                  "            if (responseData.hasOwnProperty(prop) && !Array.isArray(responseData[prop])) {",
                  "                var val = responseData[prop];",
                  "                if (typeof val === 'object' && val !== null) {",
                  "                    val = JSON.stringify(val);",
                  "                }",
                  "                tableData.push({",
                  "                    key: prop,",
                  "                    value: String(val)",
                  "                });",
                  "            }",
                  "        }",
                  "        if (tableData.length > 0) {",
                  "            payload.hasTableData = true;",
                  "            payload.tableData = tableData;",
                  "        }",
                  "    }",
                  "    ",
                  "    // Show raw JSON if no other visualization applies",
                  "    if (!payload.hasArrayData && !payload.hasTableData && !payload.message) {",
                  "        payload.showRawJson = true;",
                  "        payload.rawJson = JSON.stringify(responseData, null, 2);",
                  "    }",
                  "    ",
                  "    return payload;",
                  "}",
                  "",
                  "pm.visualizer.set(template, constructVisualizerPayload());"
