                  "    .chart-container { background: white; border-radius: 8px; padding: 20px; margin-top: 20px; }",
                  "    .json-display { ",
                  "        background: #263238; ",
                  "        color: #aed581; ",
                  "        padding: 15px; ",
                  "        border-radius: 4px; ",
                  "        overflow-x: auto; ",
                  "        font-family: monospace;",
                  "        white-space: pre-wrap;",
                  "    }",
                  "</style>",
                  "",
                  "<div class=\"container\">",
                  "    <div class=\"status-card {{statusClass}}\">",
                  "        <div class=\"status-title\">DELETE Request Result</div>",
                  "        <span class=\"status-code {{codeClass}}\">{{statusCode}} {{statusText}}</span>",
                  "        {{#if message}}",
                  "        <div class=\"message\">{{message}}</div>",
